Algoritmo "013"
// Aumento de 15% no salario
Var
    salario, aumento: Real


Inicio
      Escreval("------------------------------")
      Escreval("Qual o valor do seu salário?:")
      Escreval("------------------------------")
      Leia(salario)
      
      aumento <- salario + (salario * 0.15)
      
      Escreval("Seu salário com aumento foi:", aumento)
      
      


Fimalgoritmo