Algoritmo "072"
// Adicionar números em um vetor
Var
      num: vetor[1..10] de inteiro
      i: Inteiro


Inicio

       Escreval("---------------------------------------------------")

       Para i <- 1 ate 10 faca
       num[i] <- i * 5
              Escreva("[", num[i], "]")
       FimPara

       Escreval()
       Escreval("---------------------------------------------------")
Fimalgoritmo