Algoritmo "041"
// contagem regressiva de 100 a 0 de 5 em 5.
Var
     valor, contador : Inteiro


Inicio

contador <- 100
          Enquanto (contador >= 0) faca
               Escreval(contador)
               contador <- contador - 5
          FimEnquanto
      Escreval("Finished!")


Fimalgoritmo