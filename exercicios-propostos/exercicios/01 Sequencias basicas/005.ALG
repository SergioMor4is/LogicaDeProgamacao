Algoritmo "005"
// Calculo de Media de alunos
Var
   nota1, nota2: Inteiro
   media: Real

Inicio
    Escreval("Digite sua primeira nota:")
    Leia(nota1)
    Escreval("Digite sua segunda nota:")
    Leia(nota2)
    
    media <- (nota1 + nota2)/2
    
    Escreval("Sua média foi de ", media)
    


Fimalgoritmo