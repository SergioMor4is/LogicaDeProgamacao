Algoritmo "contador"

Var
    valor, contador : Inteiro


Inicio
      contador <- 0
      Escreva("Quer contar ate quanto?")
      Leia(valor)
          Enquanto (contador <= valor) faca
               Escreval(contador)
               contador <- contador + 1
          FimEnquanto
      Escreval("Finished!")

Fimalgoritmo